{
   "rootDir":"../../",
   "setupFiles": [
      "<rootDir>/config/test/setupTest.ts"
   ],
   "restoreMocks": true,
   "automock": false,
   "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$":"<rootDir>/config/test/fileMock.ts"
   },
   "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
   ],
   "transform": {
      ".ts": "ts-jest"
   },
   "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
   ],
   "reporters":[
      "default",
      "jest-junit"
   ],
   "coverageDirectory": "coverage",
   "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
   "coveragePathIgnorePatterns":[
      "node_modules"
   ],
   "verbose": true
}
